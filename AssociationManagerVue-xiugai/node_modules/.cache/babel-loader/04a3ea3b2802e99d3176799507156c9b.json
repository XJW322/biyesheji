AAA,EAAA,EAFA;AAGA,QAAA,MAAA,EAAA,EAHA;AAIA,QAAA,MAAA,EAAA;AAJA,OAAA;AAMA,KAzDA;AA0DA,IAAA,UA1DA,wBA0DA;AACA,WAAA,QAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,KA7DA;AA8DA,IAAA,OA9DA,qBA8DA;AAAA;;AACA,MAAA,UAAA,CAAA,KAAA,WAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,OAAA,EAAA,IAAA,CAAA,GADA;AAEA,UAAA,IAAA,EAAA;AAFA,SAAA;;AAKA,QAAA,MAAA,CAAA,WAAA,CAAA,CAAA,EAAA,MAAA,CAAA,QAAA,EAAA,MAAA,CAAA,OAAA,CAAA,KAAA;;AAEA,QAAA,MAAA,CAAA,WAAA,GAAA,KAAA;AACA,OATA;AAUA,KAzEA;AA0EA,IAAA,OA1EA,mBA0EA,EA1EA,EA0EA;AAAA;;AACA,WAAA,QAAA,CAAA,iBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,UAAA,CAAA,EAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,IAAA,CAAA,GADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;;AAKA,UAAA,MAAA,CAAA,WAAA,CAAA,CAAA,EAAA,MAAA,CAAA,QAAA;AACA,SAPA;AAQA,OAbA;AAcA;AAzFA,GAzBA;AAoHA,EAAA,OApHA,qBAoHA;AAAA;;AACA,SAAA,WAAA,CAAA,CAAA,EAAA,KAAA,QAAA,EAAA,KAAA,OAAA,CAAA,KAAA;AAEA,IAAA,YAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,MAAA,MAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AAEA,MAAA,cAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,KANA;AAOA;AA9HA,CAAA","sourcesContent":["<template>\n    <div class=\"fater-body-show\">\n        <el-card shadow=\"never\">\n            <div\n                class=\"el-card-header\"\n                slot=\"header\"\n                style=\"font-size: 26px\"\n            >\n            <i class=\"iconfont icon-r-find\" style=\"font-size: 26px\"></i>\n                信息查询\n            </div>\n            <div>\n                <el-form :inline=\"true\" :model=\"qryForm\">\n                    <el-form-item>\n                        <el-input\n                            v-model=\"qryForm.title\"\n                            placeholder=\"输入通知标题…\"\n                            autocomplete=\"off\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item>\n                        <el-input\n                            v-model=\"qryForm.teamName\"\n                            placeholder=\"输入团队名称\"\n                            autocomplete=\"off\"\n                        ></el-input>\n                    </el-form-item>\n                    <el-form-item>\n                        <el-button type=\"primary\" @click=\"getPageLikeInfo()\" style=\"font-size: 18px\"\n                            > 搜索</el-button>\n                    </el-form-item>\n                </el-form>\n            </div>\n        </el-card>\n\n        <el-card shadow=\"never\">\n            <div v-if=\"userType != 2\" slot=\"header\">\n                <el-button\n                    type=\"primary\"\n                    style=\"font-size: 18px\"\n                    @click=\"showAddWin()\"\n                    >\n                    新增</el-button\n                >\n            </div>\n            <div>\n                <el-table\n                    v-loading=\"loading\"\n                    element-loading-text=\"拼命加载中\"\n                    element-loading-spinner=\"el-icon-loading\"\n                    element-loading-background=\"rgba(124, 124, 124, 0.8)\"\n                    :data=\"pageInfos\"\n                    border\n                >\n                    <el-table-column\n                        align=\"center\"\n                        type=\"index\"\n                    ></el-table-column>\n                    <el-table-column\n                        align=\"center\"\n                        prop=\"title\"\n                        label=\"通知标题\"\n                    ></el-table-column>\n                    <el-table-column\n                        align=\"center\"\n                        prop=\"teamId\"\n                        label=\"发布社团\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-tag v-if=\"scope.row.teamName\" type=\"success\">{{\n                                scope.row.teamName\n                            }}</el-tag>\n                            <el-tag v-else type=\"warning\">系统通知</el-tag>\n                        </template>\n                    </el-table-column>\n                    <el-table-column\n                        align=\"center\"\n                        prop=\"createTime\"\n                        label=\"发布时间\"\n                    ></el-table-column>\n                    <el-table-column\n                        align=\"center\"\n                        prop=\"detail\"\n                        label=\"通知详情\"\n                    ></el-table-column>\n                    <el-table-column\n                        v-if=\"userType == 0\"\n                        align=\"center\"\n                        label=\"操作处理\"\n                        fixed=\"right\"\n\t\t\t\t\t\twidth=\"140\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-button\n                                type=\"danger\" style=\"font-size: 18px\"\n                                @click=\"delInfo(scope.row.id)\"\n                                > 删除</el-button>\n                        </template>\n                    </el-table-column>\n                    <el-table-column\n                        v-if=\"userType == 1\"\n                        align=\"center\"\n                        label=\"操作处理\"\n                        fixed=\"right\"\n\t\t\t\t\t\twidth=\"140\"\n                    >\n                        <template slot-scope=\"scope\">\n                            <el-button\n                                v-if=\"scope.row.teamId\"\n                                type=\"danger\" style=\"font-size: 18px\"\n                                @click=\"delInfo(scope.row.id)\"\n                                > 删除</el-button>\n                            <el-button v-else type=\"danger\" disabled style=\"font-size: 18px\"\n                                > 删除</el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n                <el-pagination\n                    v-if=\"pageTotal >= 0\"\n                    style=\"margin-top: 15px\"\n                    @size-change=\"handleSizeChange\"\n                    @current-change=\"handleCurrentChange\"\n                    :current-page=\"pageIndex\"\n                    :page-sizes=\"[5, 10, 20, 50]\"\n                    :page-size=\"pageSize\"\n                    layout=\"total, sizes, prev, pager, next, jumper\"\n                    :total=\"totalInfo\"\n                >\n                </el-pagination>\n            </div>\n        </el-card>\n\n        <el-dialog title=\"添加信息\" width=\"600px\" :visible.sync=\"showAddFlag\">\n            <el-form label-width=\"90px\" :model=\"noticesForm\">\n                <el-form-item label=\"通知标题\">\n                    <el-input\n                        v-model=\"noticesForm.title\"\n                        placeholder=\"请输入通知标题…\"\n                        autocomplete=\"off\"\n                    ></el-input>\n                </el-form-item>\n                <el-form-item v-if=\"userType == 1\" label=\"发布社团\">\n                    <el-select\n                        style=\"width: 100%\"\n                        v-model=\"noticesForm.teamId\"\n                        placeholder=\"请选择发布社团…\"\n                    >\n                        <el-option\n                            v-for=\"(item, index) in teams\"\n                            :key=\"index\"\n                            :label=\"item.name\"\n                            :value=\"item.id\"\n                        ></el-option>\n                    </el-select>\n                </el-form-item>\n                <el-form-item label=\"通知详情\">\n                    <el-input\n                        type=\"textarea\"\n                        rows=\"5\"\n                        v-model=\"noticesForm.detail\"\n                        placeholder=\"请输入通知详情…\"\n                        autocomplete=\"off\"\n                    ></el-input>\n                </el-form-item>\n            </el-form>\n            <div slot=\"footer\" class=\"dialog-footer\">\n                <el-button @click=\"showAddFlag = false\" style=\"font-size: 18px\"\n                    >\n                    取 消</el-button\n                >\n                <el-button\n                    type=\"primary\"\n                    @click=\"addInfo()\"\n                    style=\"font-size: 18px\"\n                    >\n                    确 定</el-button\n                >\n            </div>\n        </el-dialog>\n    </div>\n</template>\n\n<style>\n</style>\n\n<script>\nimport {\n    getManTeamList,\n    getLoginUser,\n    getPageNotices,\n    addNotices,\n    delNotices,\n} from \"../../api\";\n\nexport default {\n    data() {\n        return {\n            teams: [],\n            userType: \"\",\n            pageInfos: [],\n            pageIndex: 1,\n            pageSize: 10,\n            pageTotal: 0,\n            totalInfo: 0,\n            loading: true,\n            showAddFlag: false,\n            qryForm: {\n                token: this.$store.state.token,\n                teamName: \"\",\n                title: \"\",\n            },\n            noticesForm: {\n                id: \"\",\n                title: \"\",\n                detail: \"\",\n                teamId: null,\n            },\n        };\n    },\n    methods: {\n        getPageInfo(pageIndex, pageSize) {\n            getPageNotices(pageIndex, pageSize, this.qryForm.token).then(\n                (resp) => {\n                    this.pageInfos = resp.data.data;\n                    this.pageIndex = resp.data.pageIndex;\n                    this.pageSize = resp.data.pageSize;\n                    this.pageTotal = resp.data.pageTotal;\n                    this.totalInfo = resp.data.count;\n\n                    this.loading = false;\n                    \n                }\n            );\n        },\n        getPageLikeInfo() {\n            getPageNotices(\n                1,\n                this.pageSize,\n                this.qryForm.token,\n                this.qryForm.title,\n                this.qryForm.teamName\n            ).then((resp) => {\n                this.pageInfos = resp.data.data;\n                this.pageIndex = resp.data.pageIndex;\n                this.pageSize = resp.data.pageSize;\n                this.totalInfo = resp.data.count;\n                this.pageTotal = resp.data.pageTotal;\n                this.loading = false;\n                \n            });\n        },\n        handleSizeChange(pageSize) {\n            this.getPageInfo(\n                this.pageIndex,\n                pageSize,\n                this.qryForm.token,\n                this.qryForm.title,\n                this.qryForm.teamName\n            );\n        },\n        handleCurrentChange(pageIndex) {\n            this.getPageInfo(\n                pageIndex,\n                this.pageSize,\n                this.qryForm.token,\n                this.qryForm.title,\n                this.qryForm.teamName\n            );\n        },\n        initForm() {\n            this.noticesForm = {\n                id: \"\",\n                title: \"\",\n                detail: \"\",\n                teamId: null,\n            };\n        },\n        showAddWin() {\n            this.initForm();\n            this.showAddFlag = true;\n        },\n        addInfo() {\n            addNotices(this.noticesForm).then((resp) => {\n                this.$message({\n                    message: resp.msg,\n                    type: \"success\",\n                });\n\n                this.getPageInfo(1, this.pageSize, this.qryForm.token);\n\n                this.showAddFlag = false;\n            });\n        },\n        delInfo(id) {\n            this.$confirm(\"即将删除相关信息, 是否继续?\", \"提示\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\",\n            }).then(() => {\n                delNotices(id).then((resp) => {\n                    this.$message({\n                        message: resp.msg,\n                        type: \"success\",\n                    });\n\n                    this.getPageInfo(1, this.pageSize);\n                });\n            });\n        },\n    },\n    mounted() {\n        this.getPageInfo(1, this.pageSize, this.qryForm.token);\n\n        getLoginUser(this.$store.state.token).then((resp) => {\n            this.userType = resp.data.type;\n\n            getManTeamList(resp.data.id).then((resp) => {\n                this.teams = resp.data;\n            });\n        });\n    },\n};\n</script>"],"sourceRoot":"src/views/pages"}]}