,OAArB,EAA8BR,OAAO,CAACY,UAAtC;AACD,GAFD,MAEO;AACLZ,IAAAA,OAAO,CAACc,WAAR,CAAoBN,OAApB;AACD;AACF;;AAED,IAAM9B,OAAO,GAAG,CAAC,QAAD,CAAhB;AAEA,IAAMU,IAAI,GAAG;AACX,WAAQ,CACN,IADM,EACA,KADA,EAEN,IAFM,EAEA,KAFA,CADG;AAKX,aAAW,CACT,IADS,EACH,KADG,EACI,MADJ,CALA;AAQX,WAAS,CACP,IADO,EACD,IADC,EACK,IADL,CARE;AAWX,aAAW,CACT,IADS,EACH,IADG,CAXA;AAcX,iBAAe,CACb,IADa,CAdJ;AAiBX,aAAW,CACT,IADS,EACH,MADG;AAjBA,CAAb","sourcesContent":["/*\r\n * @Description: \r\n * @Author: Rabbiter\r\n * @Date: 2023-10-12 00:35:00\r\n */\r\nexport function toLoad(excludeInnerTexts) {\r\n  setTimeout(() => {\r\n    tagList.forEach((tag) => {\r\n      loadTag(tag, excludeInnerTexts)\r\n    })\r\n  }, 100)\r\n}\r\n\r\nfunction loadTag(tagName, excludeInnerTexts) {\r\n  const elements = document.getElementsByTagName(tagName);\r\n    for (let i = 0; i < elements.length; i++) {\r\n      for (let key in json) {\r\n        if (json.hasOwnProperty(key)) {\r\n          let value = json[key];\r\n          value.forEach((v) => {\r\n            if(excludeInnerTexts != undefined) {\r\n              for(var j = 0; j < excludeInnerTexts.length; ++j) {\r\n                if(elements[i].innerHTML.includes(excludeInnerTexts[j])) {\r\n                  return\r\n                }\r\n              }\r\n            }\r\n            if(elements[i].innerHTML.includes(v)) {\r\n              if(!hasITagWithClass(elements[i], key.replace(/%/g, \"\"))) {\r\n                updateITag(elements[i], key.replace(/%/g, \"\"))\r\n              }\r\n            }\r\n          })\r\n        }\r\n      }\r\n    }\r\n    return null;\r\n}\r\n\r\nfunction hasITagWithClass(element, suffix) {\r\n  \r\n  const temp = \"i%.%i%c%o%n%f%o%n%t%.%i%c%o%n%-%r%-\"\r\n  const iTag = element.querySelector(temp.replace(/%/g, \"\") + suffix);\r\n  return iTag !== null;\r\n}\r\n\r\nfunction updateITag(element, suffix) {\r\n  const iTags = element.querySelectorAll(\"i\");\r\n  iTags.forEach((iTag) => {\r\n    iTag.remove();\r\n  });\r\n\r\n  const newITag = document.createElement(\"i\");\r\n  const temp = \"i%c%o%n%f%o%n%t i%c%o%n%-%r%-%\"\r\n  newITag.className = temp.replace(/%/g, \"\") + suffix;\r\n  newITag.setAttribute(\"style\", \"font-size: 24px\");\r\n  if (element.firstChild) {\r\n    element.insertBefore(newITag, element.firstChild);\r\n  } else {\r\n    element.appendChild(newITag);\r\n  }\r\n}\r\n\r\nconst tagList = [\"button\"]\r\n\r\nconst json = {\r\n  \"y%e%s\":[\r\n    \"登录\", \"登 录\", \r\n    \"确定\", \"确 定\"\r\n  ],\r\n  \"l%e%f%t\": [\r\n    \"取消\", \"取 消\", \"返回登录\"\r\n  ],\r\n  \"a%d%d\": [\r\n    \"注册\", \"新增\", \"添加\"\r\n  ],\r\n  \"e%d%i%t\": [\r\n    \"修改\", \"编辑\"\r\n  ],\r\n  \"d%e%l%e%t%e\": [\r\n    \"删除\"\r\n  ],\r\n  \"f%i%n%d\": [\r\n    \"搜索\", \"显示全部\"\r\n  ]\r\n}\r\n\r\n\r\n\r\n\r\n"]}]}